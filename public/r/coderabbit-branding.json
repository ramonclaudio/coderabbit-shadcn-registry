{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "coderabbit-branding",
  "author": "Ray <hi@rmncldyo.com>",
  "type": "registry:component",
  "title": "CodeRabbit Branding",
  "description": "CodeRabbit logo and icon components with dark/light theme support. Includes SVG assets.",
  "categories": [
    "branding",
    "components"
  ],
  "dependencies": [
    "react"
  ],
  "files": [
    {
      "path": "registry/default/coderabbit/components/coderabbit-branding.tsx",
      "type": "registry:component"
    },
    {
      "path": "registry/default/coderabbit/public/coderabbit/coderabbit-dark-icon.svg",
      "type": "registry:file",
      "target": "~/public/coderabbit/coderabbit-dark-icon.svg"
    },
    {
      "path": "registry/default/coderabbit/public/coderabbit/coderabbit-light-icon.svg",
      "type": "registry:file",
      "target": "~/public/coderabbit/coderabbit-light-icon.svg"
    },
    {
      "path": "registry/default/coderabbit/public/coderabbit/coderabbit-dark-logo.svg",
      "type": "registry:file",
      "target": "~/public/coderabbit/coderabbit-dark-logo.svg"
    },
    {
      "path": "registry/default/coderabbit/public/coderabbit/coderabbit-light-logo.svg",
      "type": "registry:file",
      "target": "~/public/coderabbit/coderabbit-light-logo.svg"
    }
  ]
}